@model KalendarzKarieryData.Models.ViewModels.RegisterViewModel

<div class="title-header">
	<span>Dziennik
	</span>
</div>
<section id="detailsPanel">
	<div id="todayDateAndTimeContainer" data-bind="click: $root.showTodayInDetailsPageCalendarDetailsTable" class="hover-cursor-pointer">
		<div style="position: relative; z-index: -1;">
			<div id="todayDateDay" data-bind="text: $root.todayDate.day"></div>
			<div id="todayDateDayName" data-bind="text: $root.todayDate.getDayName()"></div>
			<div id="todayDateMonth" data-bind="text: $root.todayDate.getMonthName()"></div>
			<div id="todayDateYear" data-bind="text: $root.todayDate.year"></div>
		</div>

		<div id="clockCanvas">
			<div id="digitalClock"></div>
		</div>
	</div>

	<div class="events-menu-container">
		<div class="menu-item-container" data-bind="click: function () { $root.showSelectedEventsOnMenuItemClick( $element, $root.detailsPageSelectedEvents ) }">
			<div class="border-item inne-border-tab"></div>
			<div class="menu-item" data-eventkind="7">
				<span>Inne</span>

			</div>
			<div class="menu-item-number-container">
				<div class="menu-item-number-upcoming" data-bind="text: $root.myEventTreeCountBasedOnEventKind && $root.myEventTreeCountBasedOnEventKind[7].upcoming() != 0 ? $root.myEventTreeCountBasedOnEventKind[7].upcoming() : '-'"></div>
				@*<div class="menu-item-number-old" data-bind="text: $root.myEventTreeCountBasedOnEventKind[7] ? $root.myEventTreeCountBasedOnEventKind[7].events.old() : 0"></div>*@
			</div>
		</div>

		<div class="menu-item-container" data-bind="click: function () { $root.showSelectedEventsOnMenuItemClick( $element, $root.detailsPageSelectedEvents ) }">
			<div class="border-item lekcje-border-tab"></div>
			<div class="menu-item" data-eventkind="2">
				<span>Lekcje</span>

			</div>
			<div class="menu-item-number-container">
				<div class="menu-item-number-upcoming" data-bind="		text: $root.myEventTreeCountBasedOnEventKind && $root.myEventTreeCountBasedOnEventKind[2].upcoming() != 0 ? $root.myEventTreeCountBasedOnEventKind[2].upcoming() : '-'"></div>
