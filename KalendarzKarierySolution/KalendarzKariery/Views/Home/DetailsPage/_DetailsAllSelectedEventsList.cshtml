<div id="detailsPageAllEventsListContainer" style="display: none;">
	<div style="padding: 5px; color: rgb(150, 141, 109); border-bottom:1px solid rgb(242, 242, 242); font-size: 1.4em;">
		Mój kalendarz:
	</div>
	<div data-bind="foreach: $root.detailsPageAllSelectedEvents" style="position: relative; padding:60px 0px; left: 1px;text-align:center;">
		<!-- ko if: startDate.javaScriptStartDate > $root.todayDate.javaScriptStartDate -->
		<div>
			<div style="display: inline-block; width: 300px; margin-bottom: 2px; padding: 5px;text-align:left; vertical-align: top; color: rgb(118, 104, 75);" data-bind="text: name"></div>
			<div style="display: inline-block; color: rgb(118, 104, 75); padding: 5px;  width: 110px;" data-bind="text: startDate.displayFullDate()"></div>
			<div style="display: inline-block; color: rgb(118, 104, 75); padding: 5px;  width: 110px;" data-bind="text: startDate.displayFullTime()"></div>
			<div style="display: inline-block; padding: 5px; background: rgb(184, 168, 137); width: 100px; box-shadow: rgb(143, 125, 107) 2px 2px 10px -1px inset;" data-bind="text: kind.name, style: { color: kind.headerColor }"></div>
		</div>
		<!-- /ko -->
	</div>
</div>
