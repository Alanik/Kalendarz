<li class="list-style-none">
	<div class="event-list-item-container">
		<div class="event-list-item" data-bind="click: function () { $root.expandEventOverviewItemOnClick( $element ) }">
			<div class="rg-label expand-label">[ZWIŃ]</div>
			<div class="event-list-item-left">
				<div class="event-kind-tag" data-bind="style: { color: kind.headerColor }, text: kind.name"></div>
				<div class="list-item-date">
					<div data-bind="html: startDate.displayFullDate_EnlargeDay()"></div>
					<div data-bind="text: startDate.displayFullTime()"></div>
				</div>
				<div class="sm-label pad-top-8" data-bind="text: startDate.displayInfo()"></div>
			</div>
			<div class="event-list-item-right">
				<span class="list-item-title" data-bind="text: name"></span>
			</div>
		</div>
		<div class="event-block-expand">
			@Html.Partial( "_EventBlock", new ViewDataDictionary { { "page", ViewData["page"] } } )
		</div>
	</div>
</li>
