<section id="detailsEventBlockList">
	<div style="overflow: hidden;" data-bind="foreach: detailsPageDayEvents()">
		<div class="details-event-block-container">
		<span style="display:none;" class="hidden-event-id" data-bind="text: id"></span>
			<div class="details-event-block">
				<div data-bind="style: { color: kind.headerColor, background: 'rgb(130,130,130)', border: '1px solid rgb(221, 221, 221)', padding: '10px', marginBottom: '10px' }">
					<span data-bind="text: kind.name"></span>
				</div>
				<div class="event-block-content">
					<div><span data-bind="text: name, style: { fontSize: '1.6em', lineHeight: '1', paddingBottom: '4px' } "></span></div>
					<div style="font-size: 1.4em;"><span>godzina: </span><span data-bind="text: $root.event.startDate.display(startDate.startHour)"></span><span>:</span><span data-bind="	text: $root.event.startDate.display(startDate.startMinute)"></span><span style="padding:0 4px;"> - </span>
					<span data-bind="text: $root.event.startDate.display(startDate.endHour)"></span><span>:</span><span data-bind="	text: $root.event.startDate.display(startDate.endMinute)"></span></div>
					<div data-bind="if: address.street">
					<span>adres: </span><span data-bind="text: address.street"></span><span>, </span><span data-bind="	text: address.city"></span>
					</div>
				</div>
				<div class="details-eventblock-footer">
					<div class="details-eventblock-show-link link" data-bind="click: function () { $root.showEventInfoOnShowLinkClick($element) }"><span>pokaż</span></div>
				</div>
			</div>
			@Html.Partial("_DetailsEventInfo")
		</div>
	</div>

</section>





