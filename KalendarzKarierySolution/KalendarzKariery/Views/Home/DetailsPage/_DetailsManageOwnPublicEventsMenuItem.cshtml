<div class="display-events-overview-body" data-bind="visible: $root.detailsPageJournalMenu.selectedMenuItem() == 2">
  <div class="legend-oldorupcoming hover-cursor-pointer" data-bind="css: { open : $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showOld() }, click: function () { $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showOld( !$root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showOld() ), $root.showSelectedEvents( $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents, 'old', $root.detailsPageJournalMenu.selectedEventKindValues ) }">
    <div class="oldorupcoming-text-container">
      <span class="oldorupcoming-sign" data-bind="text: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showOld() == true ? '-' : '+'">+</span>
      <span>stare</span>
    </div>
    <span class="oldorupcoming-expand-text sm-hidden" data-bind="text: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showOld() == true ? '[zwiń]' : '[rozwiń]'"></span>
    <span class="oldorupcoming-event-count" data-bind="text: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents.old().length > 0 ? $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents.old().length : ''"></span>
  </div>
  <div class="event-list-item-container-list" data-bind="foreach: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents.old">
    <div class="event-list-item-container-list-inner-wrapper">
      <div class="event-kind-tag" data-bind="text: kind.name, style: { color: kind.headerColor }"></div>
      <div class="event-list-item-container">
        <div class="event-list-item" data-bind="click: function () { $root.expandEventOverviewItemOnClick( $element ) }">
          <div class="rg-inline-block">

            <span class="list-item-title" data-bind="text: name"></span>
          </div>
          <div class="inline-block" style="float:right;">
            <span class="list-item-date">
              <span class="list-item-other" data-bind="text: startDate.displayFullDate()"></span>
              <span class="list-item-other" data-bind="text: startDate.displayFullTime()"></span>
            </span>
          </div>
        </div>
        <div class="event-block-expand" style="display:none;">
          @Html.Partial("_EventBlock", new ViewDataDictionary { { "page", "details" } })
        </div>
      </div>
    </div>
  </div>
  <div class="legend-oldorupcoming hover-cursor-pointer" data-bind="css: { open: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showUpcoming() }, click: function () { $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showUpcoming( !$root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showUpcoming() ), $root.showSelectedEvents( $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents, 'upcoming', $root.detailsPageJournalMenu.selectedEventKindValues ) }">
    <div class="oldorupcoming-text-container">
      <span class="oldorupcoming-sign" data-bind="text: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showUpcoming() == true ? '-' : '+'">+</span>
      <span>nadchodzące</span>
    </div>
    <span class="oldorupcoming-expand-text sm-hidden" data-bind="text: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.showUpcoming() == true ? '[zwiń]' : '[rozwiń]'"></span>
    <span class="oldorupcoming-event-count" data-bind="text: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents.upcoming().length > 0 ? $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents.upcoming().length : ''"></span>
  </div>
  <div class="event-list-item-container-list" data-bind="foreach: $root.detailsPageJournalMenu.menuItems.manageOwnPublicEvents.selectedEvents.upcoming">
    <div class="event-list-item-container-list-inner-wrapper">
      <div class="event-kind-tag" data-bind="text: kind.name, style: { color: kind.headerColor }"></div>
      <div class="event-list-item-container">
        <div class="event-list-item" data-bind="click: function () { $root.expandEventOverviewItemOnClick( $element ) }">
          <div class="rg-inline-block">

            <span class="list-item-title" data-bind="text: name"></span>
          </div>
          <div class="inline-block" style="float:right;">
            <span class="list-item-date">
              <span class="list-item-other" data-bind="text: startDate.displayFullDate()"></span>
              <span class="list-item-other" data-bind="text: startDate.displayFullTime()"></span>
            </span>
          </div>
        </div>
        <div class="event-block-expand" style="display:none;">
          @Html.Partial("_EventBlock", new ViewDataDictionary { { "page", "details" } })
        </div>
      </div>
    </div>

  </div>
</div>
