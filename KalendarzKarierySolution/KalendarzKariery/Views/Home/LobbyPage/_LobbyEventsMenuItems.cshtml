			<div class="menu-item" data-eventkind="7">
				<span>Inne</span>
			</div>
			<div class="menu-item-number-container">
				<div class="menu-item-number-upcoming" data-bind="text: $root.publicEventTreeCountBasedOnEventKind[7].upcoming() != 0 ? $root.publicEventTreeCountBasedOnEventKind[7].upcoming() : '-'"></div>
			</div>
		</div>

